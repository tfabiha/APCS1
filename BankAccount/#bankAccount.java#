public class bankAccount{

    //INSTANCE VARIABLES
    private String acctName;
    private String acctPass;
    private int pin;
    private int acctNumber;
    private float balance;

    //CONSTRUCTORS
    public bankAccount(){
	 acctName = "Turtle";
	 acctPass = "password";
	 pin = 1234;
	 acctNumber = 12245;
	 balance = 0;
    }
    public bankAccount(String name, String pass, int pins, int number){
	acctName = name;
	acctPass = pass;
	pin = pins;
	acctNumber = number;
	balance = 0;
    }
    
    //METHODS
    public static void main(String[] args){
	bankAccount richard = new bankAccount();
	richard.acctInfo();
    }
    
    public void acctInfo(){
	System.out.println("Account Name: "+acctName);
	System.out.println("Account Password: "+acctPass);
	System.out.println("Account Pin: "+Integer.toString(pin));
	System.out.println("Account Number: "+Integer.toString(acctNumber));
	System.out.println("Account Balance: "+Float.toString(balance));
    }
    public void 
}
